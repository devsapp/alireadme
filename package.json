{
  "name": "ServerlessDevsComponent",
  "version": "0.0.1",
  "description": "This is a component demo for Serverless Devs Tool ",
  "autoInstall": false,
  "homepage": "https://www.serverless-devs.com",
  "author": "Serverless-Devs",
  "contributors": [],
  "license": "MIT",
  "main": "./dist/index.js",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "Serverless",
    "Framework",
    "Serverless-Devs",
    "Serverless-Tool",
    "Function Compute"
  ],
  "scripts": {
    "start": "npm run watch",
    "watch": "npm run postbuild && tsc -w",
    "prebuild": "rm -rf dist && npm i",
    "build": "esbuild src/index.ts --bundle --log-level=error --minify --platform=node --format=cjs --target=node10.4 --external:@serverless-devs/core --outfile=dist/index.js",
    "postbuild": "cp -r src/view dist/view"
  },
  "dependencies": {
    "@serverless-devs/core": "latest",
    "ejs": "^3.1.8",
    "koa": "^2.14.1",
    "koa-onerror": "^4.2.0",
    "koa-router": "^12.0.0",
    "koa-views": "^8.0.0",
    "open": "^8.4.0",
    "raw-body": "^2.5.1"
  },
  "devDependencies": {
    "@types/node": "^14.0.23",
    "esbuild": "^0.13.8",
    "rimraf": "^3.0.2",
    "ts-node": "^8.10.2",
    "typescript": "^4.4.2"
  }
}